{
  "buildCommand": "cd web && npm install && npm run build",
  "outputDirectory": "web/dist",
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python@4.3.1",
      "maxDuration": 60
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.py" },
    { "source": "/report/(.*)", "destination": "/index.html" },
    { "source": "/((?!assets|synapse-logo|favicon|_vercel).*)", "destination": "/index.html" }
  ]
}
